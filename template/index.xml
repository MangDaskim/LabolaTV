      font-weight: bold;
      color: red;
      font-size: 14px;
      animation: blink 1s infinite;
      margin-bottom: 6px;
    }

    @keyframes blink {
      0%, 100% { opacity: 1; }
      50% { opacity: 0; }
    }

    .event-title {
      font-size: 20px;
